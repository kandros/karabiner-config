{
  "title": "launch apps",
  "rules": [
    {
      "description": "right command + 1 opens emacs",
      "manipulators": [
        {
          "from": {
            "key_code": "1",
            "modifiers": {
              "mandatory": [
                "right_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open /Applications/Emacs.app"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "right command + 2 opens chrome",
      "manipulators": [
        {
          "from": {
            "key_code": "2",
            "modifiers": {
              "mandatory": [
                "right_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/Google Chrome.app'"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "right command + 3 opens iterm2",
      "manipulators": [
        {
          "from": {
            "key_code": "3",
            "modifiers": {
              "mandatory": [
                "right_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open /Applications/iTerm.app"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
